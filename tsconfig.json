{
  "compilerOptions": {
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "target": "ES2020",
    "module": "esnext",
    "sourceMap": true,
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
